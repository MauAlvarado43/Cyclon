<div class="modal fade" id="registerModal" tabindex="-1" role="dialog" aria-labelledby="registerModalLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="registerModalLabel"> <%= assets.buttons.login %> </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">

                <form>

                    <div class="form-group">
                        <label for="nameRegister"><%= assets.inputs.name %></label>
                        <input type="text" class="form-control" id="nameRegister" aria-describedby="emailHelp"
                            placeholder="example" v-model="name">
                    </div>

                    <div class="form-group">
                        <label for="lastnameRegister"><%= assets.inputs.lastname %></label>
                        <input type="text" class="form-control" id="lastnameRegister" aria-describedby="emailHelp"
                            placeholder="example" v-model="lastname">
                    </div>

                    <div class="form-group">
                        <label for="emailRegister"><%= assets.inputs.email %></label>
                        <input type="email" class="form-control" id="emailRegister" aria-describedby="emailHelp"
                            placeholder="example@example.com" v-model="email">
                    </div>

                    <div class="form-group">
                        <label for="passwordRegister"><%= assets.inputs.password %></label>
                        <input type="password" class="form-control" id="passwordRegister" placeholder="********"
                            v-model="password">
                    </div>

                    <div class="form-group">
                        <label for="confirmPasswordRegister"><%= assets.inputs.confirmp %></label>
                        <input type="password" class="form-control" id="confirmPasswordRegister" placeholder="********"
                            v-model="confirmPassword">
                    </div>

                    <button type="button" class="btn btn-success mb-3 btn-block"
                        @click="register"><%= assets.buttons.login %></button>

                </form>

            </div>
        </div>
    </div>
</div>